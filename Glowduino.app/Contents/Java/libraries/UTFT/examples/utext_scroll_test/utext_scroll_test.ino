#include <SPI.h>
#include <UTFT.h>
#include <uText.h>

extern prog_uchar CourierNewPSMT20[2026] PROGMEM;
UTFT utft(ILI9341_S5P,11,13,20,5,2);
uText utext(&utft, 320, 240);

void setup() 
{
  /* For the backlight ... */
  analogWrite(23,252);

  utft.InitLCD();
  utft.clrScr();
  utext.setFont(CourierNewPSMT20);
}

void loop() 
{
  int increment = 2;
  int16_t len = utext.getTextWidth("why",0);
  int16_t height = utext.getLineHeight();
  
  for(int x = 0; x < 320 - len; x+= increment)
  {
    /* Print the text */
    utext.print( x, 100, "why");
    
    /* Let it stay in the screen a little */
    delay(50);
    
    /* Delete the tail */
    utft.clrWindow(x,100,x+len,100+height);
  }   
}

