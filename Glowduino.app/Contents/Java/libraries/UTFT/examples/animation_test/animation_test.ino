#include <SPI.h>
#include <UTFT.h>
#include <uText.h>

extern prog_uint16_t img_1_png[1024] PROGMEM;
extern prog_uint16_t img_2_png[1024] PROGMEM;
extern prog_uint16_t img_3_png[1024] PROGMEM;
extern prog_uint16_t img_go_png[1024] PROGMEM;

UTFT utft(ILI9341_S5P,11,13,20,5,2);
uText utext(&utft, 320, 240);

void setup() 
{
  /* For the backlight ... */
  analogWrite(23,252);

  utft.InitLCD();
  utft.clrScr();
}

void loop()
{
  utft.drawBitmap(100, 100, 32, 32, img_3_png);

  delay(300);

  utft.clrWindow(100,100,132,132);

  utft.drawBitmap(100, 100, 32, 32, img_2_png);

  delay(300);

  utft.clrWindow(100,100,132,132);

  utft.drawBitmap(100, 100, 32, 32, img_1_png);

  delay(300);

  utft.clrWindow(100,100,132,132);

  utft.drawBitmap(100, 100, 32, 32, img_go_png);

  delay(3000);

  utft.clrWindow(100,100,132,132);

}

